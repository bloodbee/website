    "gatsby-plugin-netlify-cms": "^3.0.18",
    "gatsby-plugin-offline": "^3.0.32",
    "gatsby-plugin-page-transitions": "^1.0.7",
    "gatsby-plugin-react-helmet": "^3.1.22",
